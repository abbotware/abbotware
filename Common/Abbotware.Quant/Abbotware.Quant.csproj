<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <Nullable>enable</Nullable>
    <TargetFrameworks>net7</TargetFrameworks>
    <Description>Abbotware Quant / Finance / High Frequency Trading (HFT) Library</Description>
    <PackageTags>Quant;Quantitative;Finance;Hft;High Frequency Trading;</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <PackageVersion>$(Version)-beta</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Finance\Cashflows\no\ComputationalTransactions.cs" />
    <Compile Remove="Finance\Cashflows\no\TheoreticalTransactions.cs" />
    <Compile Remove="Finance\Cashflows\no\Transactions.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Finance\Cashflows\no\ComputationalTransactions.cs" />
    <None Include="Finance\Cashflows\no\TheoreticalTransactions.cs" />
    <None Include="Finance\Cashflows\no\Transactions.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Abbotware.Core\Abbotware.Core.csproj" />
  </ItemGroup>
  
</Project>
